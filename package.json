{
  "name": "example-bare",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:wrangler\"",
    "dev:vite": "vinxi dev --port 3001",
    "dev:wrangler": "wrangler --cwd .output dev",
    "migrate:local": "npx wrangler d1 migrations apply DB --local",
    "migrate:prod": "tsx ./scripts/generateSchema.ts && npx wrangler d1 migrations apply DB --remote",
    "build:schema": "tsx ./scripts/generateSchema.ts && npm run migrate:local",
    "build": "vinxi build",
    "preview": "vinxi build && dev:wrangler",
    "start": "vinxi start",
    "test": "vitest"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@solidjs/router": "0.15.4",
    "@solidjs/start": "1.2.0",
    "solid-js": "1.9.10",
    "unocss": "66.5.0",
    "vinxi": "0.5.8",
    "vite-plugin-solid": "2.11.10",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251218.0",
    "@unocss/preset-attributify": "66.5.0",
    "concurrently": "^9.2.1",
    "jsdom": "27.2.0",
    "sass-embedded": "1.91.0",
    "tsx": "^4.21.0",
    "vitest": "3.2.4",
    "wrangler": "^4.55.0"
  },
  "engines": {
    "node": ">=22"
  },
  "web-types": {
    "contributions": {
      "types-syntax": "typescript",
      "html": {
        "tags": [
          "x-input"
        ]
      }
    }
  }
}
